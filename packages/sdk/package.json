{
  "name": "@ethos/reputation-gate",
  "version": "1.0.0",
  "description": "Gate access and payments by Ethos reputation score",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "dependencies": {
    "react": "^18.2.0",
    "wagmi": "^2.5.0",
    "viem": "^2.7.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "wagmi": "^2.0.0",
    "viem": "^2.0.0"
  }
}
